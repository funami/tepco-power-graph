<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="ja">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>東京電力消費電力量グラフ - Top</title>
        <link rel="stylesheet" href="css/main.css" type="text/css" media="screen,tv">
        <script type='text/javascript' src='https://www.google.com/jsapi'></script>
        <script type='text/javascript'>
            google.load('visualization', '1', {'packages':['annotatedtimeline']});
            google.setOnLoadCallback(drawChart);
            function drawChart() {
                var data = new google.visualization.DataTable();
                data.addColumn('datetime', 'Date');
                data.addColumn('number', '消費電力');
                data.addColumn('number', '供給可能最大電力');
                data.addRows([
        [%FOREACH a=obj%]

                        [new Date([% a.year %], [% a.month %] ,[% a.day %],[% a.hour %],0,0), [% a.usage %], [% a.capacity %]],
        [%END%]
                        [new Date(2011, 2 ,28), 0,0]
                        ]);

                var chart = new google.visualization.AnnotatedTimeLine(document.getElementById('chart_div'));
                chart.draw(data, {displayAnnotations: true});
            }
        </script>
    </head>

    <body>
        // Note how you must specify the size of the container element explicitly!
        <div id='chart_div' style='width: 700px; height: 240px;'></div>

    </body>
</html>

